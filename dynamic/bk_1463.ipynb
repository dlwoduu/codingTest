   "source": [
    "N = int(input())\n",
    "\n",
    "d = [None] * (N + 1)\n",
    "\n",
    "d[N] = 0\n",
    "for i in range(N-1,0,-1):\n",
    "    if i * 3 <= N:\n",
    "        d[i] = min(d[i*3], d[i*2], d[i+1]) + 1\n",
    "    elif i * 2  <= N :\n",
    "        d[i] = min(d[i*2], d[i+1]) + 1\n",
    "    else:\n",
    "        d[i] = d[i+1] + 1\n",
    "    \n",
    "print(d[1])"
   ]
