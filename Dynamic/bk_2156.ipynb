    "N = int(input())\n",
    "\n",
    "wines = [ int(input()) for _ in range(N)]\n",
    "\n",
    "d = [wines[0], wines[0] + wines[1]]\n",
    "d.append(max(wines[0], wines[1]) + wines[2])\n",
    "\n",
    "for i in range(3, N):\n",
    "    d.append(max(d[i-2] + wines[i], d[i-3] + wines[i-1] + wines[i]))\n",
    "    \n",
    "print(max(d[N-1],d[N-2]))"
   ]
